let data=[5,7,8,6]
//rest operator
functionDemo(...args)
{
    let sum =0;
    for(const item of args)
    {
        sum+=item;
    }
    return sum;
}
console.log(restDemo(5,7));
console.log(restDemo(5,7,8));
console.log(restDemo(5,6,7,8));
spread
function spreadDemo(b,c)
{
    console.log("invoked")
    return b+c;
}
 var array=[5,6,8];
 console.log(spreadDemo(2,3));
 console.log(spreadDemo(array));
 console.log(spreadDemo(...array));
